{
  "name": "project-resume-1",
  "private": true,
  "workspaces": [
    "apps/frontend",
    "apps/backend",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev --workspace apps/frontend\" \"npm:start:dev --workspace apps/backend\"",
    "frontend": "npm run dev --workspace apps/frontend",
    "backend": "npm run start:dev --workspace apps/backend",
    "lint": "turbo lint",
    "build": "turbo build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "turbo": "^2.1.3"
  },
  "packageManager": "npm@10.8.2"
}
